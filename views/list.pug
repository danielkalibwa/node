extends layout
block content
    header.card-header
        h4.card-title Registered users

    table.table 
        thead.thead-dark
            tr
                th Firstname
                th Lastname
                th password
                th Email
                th gender
                th city

            tbody
                each user in users
                    tr
                        td #{user.firstname}
                        td #{user.lastname}
                        td #{user.password}
                        td #{user.Email}
                        td #{user.gender}
                        td #{user.city}